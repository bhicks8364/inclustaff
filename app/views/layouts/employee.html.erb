<html>
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title><%= page_title(@current_agency.name) %>	</title>
    <script src="http://www.google.com/jsapi"></script>
    <script src="ck/static/chartkick.js"></script>

    <script src="//maps.google.com/maps/api/js?v=3.18&sensor=true&client=&key=&libraries=geometry&language=&hl=&region="></script>
    <script src="//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js"></script>
    <script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/infobox/1.1.9/src/infobox_packed.js' type='text/javascript'></script> <!-- only if you need custom infoboxes -->

    <%= Gon::Base.render_data %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= favicon_link_tag 'incluStaff_twitter_logo.png' %>
    <%= javascript_include_tag 'application', "//www.google.com/jsapi", "chartkick", "charts" %>

    <%= csrf_meta_tags %>
  </head>

  <body>
    <%= render 'layouts/google_analytics' %>
    <%= render 'employee/shared/navbar' if current_user.role != "Candidate" %>
    <%= render 'employee/shared/timeclock_navbar' if current_user.role == "Candidate" %>
    
    <div class="" id="wrapper">
      
      
      <%= render 'demo/employee_how_to' if @current_agency.present?  %>
      <!-- Sidebar -->
      <%= render 'shared/employee_sidebar' %>
      
      <!-- /#sidebar-wrapper -->
      <!-- Page Content -->
      <div id="page-content-wrapper">
        
          <div class="container-fluid">
            <%= render 'layouts/alerts' %>
              <div class="row">
                <%= render 'demo/demo_nav' if current_user.role != "Candidate" && @current_agency.demo? %>
                  <div class="col-lg-12">
                      <%= yield %>
                  </div>
              </div>
          </div>
      </div>
      <!-- /#page-content-wrapper -->
    </div>
    <!-- /#wrapper -->
        <!-- Menu Toggle Script -->
    <script>
    $("#menu-toggle").click(function(e) {
        e.preventDefault();
        $("#wrapper").toggleClass("toggled");
        $("#sidebar-icon").toggleClass("fa-rotate-180 toggled");
    });
    </script>
  </body>
</html>
