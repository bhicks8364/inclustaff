	<div class="row">
		<div class="col-lg-12">
			<div class="main-box clearfix">
				<div class="clearfix">
					<h2 class="pull-left">Last orders</h2>
					
					<div class="filter-block pull-right">
						<div class="form-group pull-left">
							<input type="text" class="form-control" placeholder="Search...">
							<i class="fa fa-search search-icon"></i>
						</div>
						
						<%= link_to "<i class='fa fa-eye fa-lg'></i> View all orders".html_safe, admin_orders_path, class: "btn btn-primary pull-right" %>
					
					</div>
				</div>
				
				<div class="table-responsive clearfix">
					<table class="table table-hover">
						<thead>
							<tr>
								<th><a href="#"><span>Order ID</span></a></th>
								<th><a href="#" class="desc"><span>Date</span></a></th>
								<th><a href="#" class="asc"><span>Customer</span></a></th>
								<th class="text-center"><span>Status</span></th>
								<th class="text-right"><span>Price</span></th>
								<th>&nbsp;</th>
							</tr>
						</thead>
						<tbody>
							<% @orders.each do |order| %>
							<tr>
								<td>
									<%= link_to "#00#{order.id}", admin_order_path(order) %>
								</td>
								<td>
									<%= order.created_at.strftime('%x') %>
								</td>
								<td>
									<%= link_to order.company_name, admin_company_path(order.company) %>
								</td>
								<td class="text-center">
									<% if order.urgent? %>
									<span class="label label-danger">URGENT</span>
									<% end %>
									<% if order.needs_attention? %>
									<span class="label label-warning">Attn!</span>
									
									<% end %>
									
								</td>
								<td class="text-right">
									<%= number_to_currency(order.current_timesheets.sum(:total_bill)) %>
								</td>
								<td class="text-center" style="width: 15%;">
									<%= link_to(admin_order_path(order), class: "table-link") do %>
										<span class="fa-stack">
											<i class="fa fa-square fa-stack-2x"></i>
											<i class="fa fa-search-plus fa-stack-1x fa-inverse"></i>
										</span>
									<% end %>
								</td>
							</tr>
							<% end %>
							
						</tbody>
					</table>
				</div>
				
			</div>
		</div>
