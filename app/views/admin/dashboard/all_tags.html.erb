<div class='col-md-12 lightbox-long'>
  <h1 class='text-center'> <%= @orders.count %> open job orders &
   <%= @employees.count %> available employees.
   </h1>
    <div class='col-md-4'>
        <%= render 'admin/infoboxes/taggings' %>
    </div>
     
    <div class='col-md-4 well'>
    <h4>Open Job Orders</h4>
    <%= div_for(@orders.needs_attention) do |order| %>
    <%= render order %>
    <% end %>
</div>

    <div class='col-md-4 well'>
        <h4>Available Employees</h4>
        <%= div_for(@employees.available) do |employee| %>
        <%= render employee %>
           
            
        <% end %>
    </div>
  





</div>