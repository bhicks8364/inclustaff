<div class="col-md-4">
    
    <h5 class='text-center'>Total Orders: <%= admin.job_orders.count %></h5>
 
        <%= pie_chart admin.job_orders.group(:company).count, min: 1000, max: 5000 , library: {legend: {position: "bottom"}, is3D: true, title: "Orders by Mark Up"} %>
        
</div>
<div class="col-md-8">
    <h2><%= admin.first_name %>'s Companies</h2>
    <%= render 'admin/shared/company_collapse', companies: admin.companies.distinct %>
  
</div>
<div class="col-md-4">
   
    <h5 class='text-center'>New Companies by Day</h5>
 
        <%= line_chart @current_agency.companies.by_account_manager(admin.id).group_by_day('companies.created_at').count %>
        
</div>
