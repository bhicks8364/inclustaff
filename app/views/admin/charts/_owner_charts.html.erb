

<div class="col-md-12">
    <%= line_chart @current_agency.companies.includes(:invoices).map{|company|
    {name: company.name, data: company.invoices.group_by_week(:created_at, range: 2.weeks.ago.beginning_of_week.midnight..Time.now.beginning_of_week).sum(:total)}
}, library: {backgroundColor: "#e5dbcf", legend: {position: "right"}, title: "Company Billings by Company"} %>
</div>