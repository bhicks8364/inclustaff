<div class='col-md-12'>
  <div class='col-md-12'>
    <%= line_chart @current_agency.companies.order('name').map{|company|
        {name: company.name, data: company.invoices.group_by_week('created_at').sum(:total)}
    }, library: {backgroundColor: "#eee", legend: {position: "bottom"}, title: "Company Billings by Week"} %>
  </div>
  <div class='col-md-6'>
    <%= column_chart Admin.account_managers.map{|admin|
      {name: admin.name, data: admin.account_orders.includes(:jobs => :current_timesheet).group(:account_manager).sum(:total_bill)}
  } %>
  </div>
  <div class='col-md-6'>
    <%= column_chart Admin.account_managers.map{|admin|
      {name: admin.name, data: admin.account_orders.includes(:jobs => :timesheets).group(:account_manager).sum(:total_bill)}
  } %>
  </div>
  <div class='col-md-6'>
    <%= column_chart Admin.recruiters.map{|admin|
      {name: admin.name, data: admin.jobs.includes(:current_timesheet).group(:recruiter).sum(:total_bill)}
  } %>
  </div>
</div>

  
