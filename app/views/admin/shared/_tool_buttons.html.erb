<div class="btn-toolbar" role="toolbar" aria-label="...">
  <div class="btn-group" role="group" aria-label="...">
    <%= link_to(admin_timesheet_path(timesheet), class: "table-link" ) do %>
					<span class="fa-stack">
						<i class="fa fa-square fa-stack-3x fa-inverse"></i>
						<i class="fa fa-folder-open fa-stack-1x "></i>
					</span>
				<% end %>
				
				<%= link_to(edit_admin_timesheet_path(timesheet), class: "table-link" ) do %>
					<span class="fa-stack">
						<i class="fa fa-square fa-stack-3x fa-inverse"></i>
						<i class="fa fa-pencil fa-stack-1x "></i>
					</span>
				<% end %>

				<%= link_to(admin_timesheet_path(timesheet), method: :delete, data: { confirm: 'Are you sure?' }) do %>
					<span class="fa-stack">
						<i class="fa fa-square fa-stack-3x fa-inverse"></i>
						<i class="fa fa-trash-o fa-stack-1x "></i>
					</span>
				<% end %>
  </div>
  <div class="btn-group" role="group" aria-label="...">...</div>
  <div class="btn-group" role="group" aria-label="...">...</div>
</div>