<%= simple_form_for [:admin, @company] do |f| %>
    <div class='form-group'>
        <%= f.input :name %>
    </div>
    <div class='form-group'>
        <%= f.input :address %>
    </div>
    <div class='form-group'>
        <%= f.input :city %>
    </div>
    <div class='form-group'>
        <%= f.input :state %>
    </div>
    <div class='form-group'>
        <%= f.input :zipcode %>
    </div>
    <div class='form-group'>
        <%= f.input :contact_name %>
    </div>
    <div class='form-group'>
        <%= f.input :contact_email %>
    </div>

    <div class='form-actions'>
         <%= f.submit %>
    </div>

   

<% end %>
