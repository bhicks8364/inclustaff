

<div class="col-md-12 well form-group nested-fields">
<div class='col-md-12'>


    <%= f.input :state, disabled: true, label: false, wrapper_html: {class: 'col-md-10'} %>
        <%= link_to_remove_association '<i class="fa fa-trash red fa-1x"></i>'.html_safe, f, class: 'btn btn-tiny'%>
</div>
    <div class='center'>
  Time In:
        <%= f.input :time_in, as: :datetime, label: false, ampm: true, order: [:month, :day, :hour, :minute], wrapper_html: {class: 'form-inline center'}, input_html: {class: 'form-control'} %>

   Time Out:
       <%= f.input :time_out, as: :datetime, label: false, ampm: true, order: [:month, :day, :hour, :minute], wrapper_html: {class: 'form-inline center'}, input_html: {class: 'form-control '} %>
        </div>


</div>