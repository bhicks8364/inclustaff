<div class="btn-toolbar" role="toolbar" aria-label="...">
    <div class="btn-group" role="group" aria-label="...">
      <%= link_to(admin_timesheet_path(timesheet), data: {toggle: "tooltip", placement: "top", title: "View Timesheet"}, class: "" ) do %>
					  <%= timesheet.week_ending %>
			<% end %>
    </div>
    	<% if !timesheet.current? %>
    <div class="btn-group" role="group" aria-label="...">
      <a class='' data-behavior="timesheet-toggle" data-toggle="tooltip" data-placement="left" title="<%= timesheet.approved? ? "Unapprove" : "Approve"%>">
			  <span class="" data-behavior="approve-button">
		          <% if timesheet.approved? %>
    					  <i class="fa fa-times fa-1x"></i>
		          <% else %>
			          <i class="fa fa-check fa-1x"></i>
		          <% end %>
		    </span>
		  </a>
    </div>
    <% end %>
    <div class="btn-group" role="group" aria-label="...">
      <%= link_to(admin_timesheet_path(timesheet, format: :pdf), data: {toggle: "tooltip", placement: "top", title: "Download PDF" }) do %>
  				  <i class="fa fa-download fa-1x"></i>
  			<% end %>
    </div>
</div>