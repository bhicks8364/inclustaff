<% if current_page?(controller: 'shifts', action: 'index') %>
      <%= link_to shift.employee.last_name, admin_shift_path(shift) %>
      <% else %>
      <%= link_to shift.employee.name, admin_shift_path(shift) %>
      <% end %><br>
<%= shift.time_in.stamp("3:00 AM") %> - <%= shift.time_out? ? shift.time_out.stamp("3:00 AM") : shift.state %>