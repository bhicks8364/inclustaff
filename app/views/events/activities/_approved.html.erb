<%= div_for event do %>
<i class="fa fa-check fa-fw"></i>
	<%= event.company_admin.try(:name) || event.admin.try(:name) %>
    <strong>	<%= event.action.humanize.downcase %></strong>
    a <%= event.eventable_type.downcase %> for <%=  signed_in_link_to event.eventable.name, event.eventable %>
		<span class='label label-success black pull-right'>
		    <%= event.created_at.stamp('11/13 at 8:30am') %>
		</span>

<% end %>