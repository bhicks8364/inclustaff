<%= link_to event_path(event), class: 'list-group-item list-group-item-success' do %>
	 <%= event.company_admin.try(:name) || event.admin.try(:name) %>
	 <%= event.eventable.employee.try(:name) || "" %>
   <strong>	<%= event.action.humanize.downcase %></strong> <%= event.eventable.name %> 
    	 

		<span class='label label-success black pull-right' data-toggle="tooltip" title="Current Status">
		     <small> <%= event.created_at.stamp('11/13 at 8:30am') %></small>
		  
		</span>

<% end %>

