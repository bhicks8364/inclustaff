<div class='col-md-12 bg-white'>
  <div class='col-md-6'>
    <%= render 'shared/searching_between' %>
  </div>
 
  <div class='col-md-6 form-inline well well-sm text-center'>
     <%= render 'events/search' %>
  </div>
  
  <div class='col-md-6 list-group'>
    <li class="list-group-item"><div class='text-center'><i class="fa fa-file-text-o fa-fw"></i><strong>&nbsp; Agency Events</strong></div></li>
  
    <% @events.all.each do |event| %>
      <% if lookup_context.template_exists?(event.action, "events/activities", true) %>
        <%= render partial: "events/activities/#{event.action}", locals: {event: event} %>
      <% end %>
    <% end %>
  </div>
  <div class='col-md-6 list-group'>
     <li class="list-group-item"><div class='text-center'><i class="fa fa-file-text-o fa-fw"></i><strong>&nbsp; Company Events</strong></div></li>
   
  
    <% @events.company_admin.each do |event| %>
      <% if lookup_context.template_exists?(event.action, "events/activities", true) %>
        <%= render partial: "events/activities/#{event.action}", locals: {event: event} %>
      <% end %>
    <% end %>
  </div>
  <div class='col-md-6'>
    <h1>Applications</h1>
    <%= render 'events/applications' %>
  </div>
</div>