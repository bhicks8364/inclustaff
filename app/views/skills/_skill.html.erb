<%= div_for skill do %>
    <% if skill.job_order? && skill.required? %>
    <i class='fa fa-star'></i><% elsif skill.job_order? %>
    <i class='fa fa-star-o'></i>
    <% end %>
    
    <strong><%= skill.name %></strong>
    <%= link_to "<i class='fa fa-trash-o red pull-right'></i>".html_safe, skill, method: :delete, remote: true %>
   
<% end %>