<div class='col-md-12 '>
  <div class='col-md-3'>
  </div>
  <div class='col-md-6 well well-sm'>
    <h1 class='text-center'>Sign Up</h1>
<%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= f.error_notification %>
  <div class="form-inputs">
      <%= f.input :first_name, required: true, placeholder: "First Name", label: false, autofocus: true %>
      <%= f.input :last_name, required: true, placeholder: "Last Name", label: false %>
      <%= f.input :address, required: true, placeholder: "Street Address", label: false %>
      <%= f.input :city, required: true, placeholder: "City", label: false %>
      <%= f.input :state, required: true, placeholder: "State", label: false %>
      <%= f.input :zipcode, required: true, placeholder: "Zipcode", label: false %>
      <%= f.input :email, required: true, placeholder: "Email  Address", label: false  %>
      <% if !signed_in? %>
      <%= f.input :password, placeholder: "Password", label: false, hint: ("#{@minimum_password_length} characters minimum" if @minimum_password_length) %>
      <%= f.input :password_confirmation, placeholder: "Password Confirmation", label: false %>
      <% end %>
  <div class="form-actions">
    <%= f.button :submit, "Save" %>
  </div>
<% end %>

  </div>
  <div class="pull-right">
    <%= render "devise/shared/links" %>
  </div>

</div>
<% if admin_signed_in? %>
  <div class='col-md-6'>
  	<%= form_for @import, url: import_users_path, multipart: true do |f| %>
									
									<% if @import.errors.full_messages.each do |msg| %>
									<div class='alert alert-danger'>
									<div><%= msg %></div>
										<% end %>
									</div>
								<% end %>
								<div class='form-field'>
								  <%= f.file_field :file %>
								</div>
									<div class='form-actions'>
									  <%= f.submit "Upload" %>
									</div>
									
								<% end %>
  </div>
<% end %>


</div>