<div class='col-md-12 dark'>


<div class='col-md-4 col-md-offset-4 dark form-group'>
    <h2 class='text-center'>Admin Sign In</h2>
  <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name), defaults: { input_html: { class: 'form-control' } })  do |f| %>
 
    <%= f.input :email, required: false, autofocus: true, placeholder: "Email Address", input_html: {class: 'form-group'}, label: false  %>
    <%= f.input :password, required: false, placeholder: "Password", input_html: {class: 'form-group'}, label: false %>
    <%= f.input :remember_me, as: :boolean, inline_label: "Remember me",input_html: {class: 'boolean'}, wrapper_html: {class: 'checkbox'} if devise_mapping.rememberable? %>

    <%= f.button :submit, "Sign In", class: "btn btn-primary btn-lg btn-block" %>

<% end %>
</div>



</div>