<a class="btn btn-default btn-sm" role="button" data-toggle="collapse" href="#companyContact" aria-expanded="false" aria-controls="companyContact">
  <%= @company.name %>'s Contact Info
</a>

<div class="collapse" id="companyContact">
  <div class="well">
    <address>
      <strong><%= link_to @company.name, @company %></strong><br>
      <%= @company.address %><br>
      <%= @company.city %>, <%= @company.state %> <%= @company.zipcode %><br>
      <abbr title="Phone">P:</abbr> (123) 456-7890
    </address>

    <address>
      <strong><%= @company.contact_name %></strong><br>
      <a href="mailto:#"><%= @company.contact_email %></a>
    </address>


  </div>
  <%= link_to 'Edit Company', edit_company_path(@company) %>
</div>