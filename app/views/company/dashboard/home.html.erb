	<div class='col-md-12'>
    <% Calendar.new events: @company.shifts do |date, shifts| %>
  <%= date %>

  <% shifts.each do |shift| %>
    <div>
      <%= shift.name %>
    </div>
  <% end %>
<% end %>
  </div>
<div class='col-md-12'>
    <h2 class='page-header white'> Welcome, <%= current_company_admin.name %>!</h2>
 	<div class='col-md-2'>
	    <div class="btn-group-vertical" role="group" aria-label="...">   
	        <%= link_to "Timeclock", company_timeclock_path, class: 'btn btn-primary'%>
	        <%= link_to "Jobs", company_jobs_path, class: 'btn btn-primary'%>
	        <%= link_to "Edit Company Admin", edit_company_admin_registration_path, class: 'btn btn-primary'%>
	        <%= link_to "Clock Out All", clock_out_all_company_jobs_path, method: :patch, class: 'btn btn-primary' if @on_shift.any? %>
	    </div>
    </div>
	<div class='col-md-6'>
	    
	   
	</div>
	<div class='col-md-4'>
		<h2 class='white'>  <%= pluralize( @current_company_admin.company.invoices.unpaid.count, 'unpaid invoice') %></h2>
		 <%= div_for @company.invoices.unpaid.order(created_at: :desc), class: "lightbox black fa-ul" do |invoice| %>
	        <%= render "company/invoices/invoice", invoice: invoice %>
	    
	    <% end %>
		<div class='scroll400'>
			<%= div_for @company.invoices.paid.order(date_paid: :desc), class: "lightbox black fa-ul" do |invoice| %>
			        <%= render "company/invoices/invoice", invoice: invoice %>
			<% end %>
		</div>
	</div>
</div>