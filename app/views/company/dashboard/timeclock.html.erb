 <h1 class='page-header'><%= @company.name %>'s Timeclock</h1>
<div class='col-md-1'>
</div>
<div class='col-md-10'  data-behavior='company-job-list'>
    <div class='col-md-5 well well-sm'>
         <h2>Clocked In</h2>
         <div class='text-center' id='clocked_in_list'>
        <%= div_for(@clocked_in.order('employees.last_name').each, class: '') do |job| %>
              <%= render 'company/jobs/job', job: job %>
        <% end %>
        </div>
    </div>
    <div class='col-md-1'>
    </div>
    <div class='col-md-5 well well-sm'>
         <h2>Clocked Out</h2>
         
         <div class='text-center' id='clocked_out_list'>
            <%= div_for(@clocked_out.order('employees.last_name').each,  class: '') do |job| %>
                  <%= render 'company/jobs/job', job: job %>
            <% end %>
         </div>
    </div>
   
   
</div>
