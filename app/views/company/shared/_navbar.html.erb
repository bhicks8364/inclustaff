<header class="navbar" id="header-navbar">
	<div class="container">
		<div class="navbar-header">
	      
	      <% if @current_agency.present? %>
			<%= link_to root_url, id: "logo", class: "navbar-brand" do %>
				<%= @current_agency.name.titleize %>	
			<% end %>
		<% else %>
			<%= link_to root_url, id: "logo", class: "" do %>
			       <%= image_tag("incluStaff_logo.png", alt: "Inclustaff") %>
			 <% end %>
		<% end %>
	   
			<%= @current_company_admin.name.titleize %>
		<button class="navbar-toggle" data-target=".navbar-ex1-collapse" data-toggle="collapse" type="button">
			<span class="sr-only">Toggle navigation</span>
			<span class="fa fa-bars"></span>
		</button>
		
		 </div>
		<div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
	       <ul class="nav navbar-nav navbar-right">
	       
	        	
				<li><%= link_to destroy_company_admin_session_path, method: :delete do %>
						<i class='fa fa-power-off'></i>
					<% end %>
				</li>
	      </ul>
		
      
    	</div>
	</div>
</header>
