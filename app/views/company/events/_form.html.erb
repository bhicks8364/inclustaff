<%= simple_form_for(@event) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :admin_id %>
    <%= f.input :action %>
    <%= f.input :eventable_id %>
    <%= f.input :eventable_type %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
