<div class="">

	<%= month_calendar(attribute: :due_by, events: @invoices) do |date, invoices| %>
	
  <div class='cal-date'><%= date %></div>

  <% invoices.each do |invoice| %>
    <div class=''>
      
      <div class=''>
        <%= inv_sym(invoice) %> <%= link_to "Invoice #: #{invoice.id}", company_invoice_path(invoice), class: 'black'%>

      </div>
      
    </div>
  <% end %>
<% end %>

</div>