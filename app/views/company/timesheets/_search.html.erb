<%= search_form_for [:company, @q] do |f| %>


  <%= f.search_field :employee_first_name_or_employee_last_name_cont, placeholder: "Search by employee" %>

  <%= f.button do %>
      <i class="fa fa-search"></i>
    <% end %>
<% end %>
<%= content_tag :table, class: "table table-hover" do %>
  <%= content_tag :th, sort_link(@q, :week) %>
  <%= content_tag :th, sort_link(@q, :state) %>
  <%= content_tag :th, sort_link(@q, :employee_first_name) %>
  <%= content_tag :th, sort_link(@q, :employee_last_name) %>
  <%= content_tag :th, sort_link(@q, :job_title) %>
  <%= content_tag :th, sort_link(@q, :total_pay) %>
<% end %>