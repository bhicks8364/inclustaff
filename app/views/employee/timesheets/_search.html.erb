<%= search_form_for [:employee, @q] do |f| %>


  <%= f.search_field :week_cont, placeholder: "Search by job" %>

  <%= f.button do %>
      <i class="fa fa-search"></i>
    <% end %>
<% end %>
<%= content_tag :table, class: "table table-hover" do %>
  <%= content_tag :th, sort_link(@q, :week) %>
  <%= content_tag :th, sort_link(@q, :state) %>
  <%= content_tag :th, sort_link(@q, :job_title) %>
  <%= content_tag :th, sort_link(@q, :total_pay) %>
<% end %>