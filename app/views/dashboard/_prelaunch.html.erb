  <%= simple_form_for @inquiry, html: {id: "prelaunch", class: 'form-inline'} do |f| %>
    <%= f.error_notification %>
      <%= f.input :name, placeholder: "Full Name", label: false, autofocus: true, input_html: {class: "form-control"} %>
      <%= f.input :email, placeholder: "Email Address", label: false %>
      <%= f.input :agency_name, placeholder: "Company Name", label: false %>
    <%= f.submit 'Tell Me More', class: 'btn btn-success inline' %>
  <% end %>
  
  