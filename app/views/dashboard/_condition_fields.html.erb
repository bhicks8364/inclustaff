<div class="field">
  <%= f.attribute_fields do |a| %>
    <%= a.attribute_select collection: @orders.ransackable_attributes, class: "form-control" %>
  <% end %>
  <%= f.predicate_select collection: @orders.ransortable_attributes, class: "form-control" %>
  <%= f.value_fields do |v| %>
    <%= v.text_field :value, class: "form-control" %>
  <% end %>
  <%= link_to "<i class='fa fa-minus-square'></i>".html_safe, '#', class: "remove_fields" %>
</div>