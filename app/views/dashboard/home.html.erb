 <div class='dark col-md-12'>
<% if admin_signed_in? %>
   
       <h1> <%= @current.name %></h1>
       <h2>Welcome, <%= @current_admin.name %><small><%= @current_admin.role %></small></h2>
       
        <%= render 'admin/infoboxes/infoboxes' %>
        
        <% if @current_admin.owner? %>
       <%= render 'admin/infoboxes/owner_charts' %>
        <% elsif @current_admin.account_manager? %>
        <% elsif @current_admin.payroll? %>
       
        <%= render 'admin/infoboxes/payroll_boxes' %>
        <% elsif @current_admin.recruiter? %>
        <h2><%= @current_admin.role %></h2>
         <%= link_to "Recruiter Dashboard", admin_recruiter_path, class: 'btn btn-primary' %>
        <% end %>
           
       
        
  
    <% end %>
    
    <% if user_signed_in? %>
   
        <div class='col-md-6'>
            <h4>Add A New Work Record</h4>
            <%= render 'work_histories/form' %>
        </div>
        <div class='col-md-6 pull-right'>
            <h4>Your Work History</h4>
            <% if @employee.work_histories.any? %>
                <%= render 'work_histories/table' %>
            <% end %>
        </div>
   
        
    <% end %>
    
    

    
    
    
    <!--EMPLOYEE HOME PAGE VIEW-->
    
<% if @employee.present? && @employee.assigned? %>
<%= render 'dashboard/employee' %>


<% end %>


<% if @current_company_admin.present? %>
	<%= link_to "<i class='fa fa-user-plus fa-fw pull-left'></i> Company Admin Sign Up ".html_safe, edit_company_admin_registration_path, class: 'btn btn-primary  btn-lg btn-block' %>



<% end %>








<% if current_user.nil? && current_admin.nil? %>
        
    

  <%= render 'test' %>
    

<% end %>


    
    





  </div>
  
